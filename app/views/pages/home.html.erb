  
<%if user_signed_in?%>
<h1><%= "welcome back!" %></h1>
<%else%>
<div class="hero-unit">
<h1>Hello, Welcome aboard </h1>
</div>
<p>
  <%= link_to "Login", new_user_session_path, class: "btn btn-primary btn-large"%>
  <%= link_to "Sign Up", new_user_registration_path, class: "btn btn-primary btn-large"%>
</p>

<%end%>
<table class="imagetable">
<!-- <table style="width:300px"> -->
  <thead>
    <tr>
      <th><font size="5" color="black">Train name</font></th>
      <th><font size="5" color="black">Status</font></th>
      
      <!-- <th colspan="3"></th> -->
    </tr>
  </thead>
<div class="hero-unit">
<h3><%=Service.last.created_at%></h3>
</div>
  <tbody>
    <% Service.all.each do |element|%><tr>
    <td><b><font size="3" color="black"><%=  element.name %></font></b></td>
    <td><b><font size="3" color="black"><%=  element.traffic %></font></b></td> 
    
      </tr>
    <% end %>
  </tbody>
</table>


